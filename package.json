{
  "name": "atom-markdown-katex",
  "main": "./lib/atom-markdown-katex",
  "version": "0.3.9",
  "description": "Markdown preview with KaTeX support. Easy export to pdf/html",
  "activationCommands": {
    "atom-workspace": [
      "markdown-katex-preview:toggle",
      "markdown-katex-preview:customize-css"
    ]
  },
  "repository": "https://github.com/shd101wyy/atom-markdown-katex",
  "license": "MIT",
  "engines": {
    "atom": ">=0.174.0 <2.0.0"
  },
  "scripts": {
    "build": "babel ./lib --watch --out-dir ./build"
  },
  "dependencies": {
    "async": "^1.5.0",
    "cheerio": "^0.19.0",
    "highlight.js": "^9.1.0",
    "katex": "^0.5.1",
    "less": "^2.5.3",
    "remarkable": "^1.6.1",
    "temp": "^0.8.3"
  }
}
