      "markdown-katex-preview:customize-css"
    ]
  },
  "repository": "https://github.com/shd101wyy/atom-markdown-katex",
  "license": "MIT",
  "engines": {
    "atom": ">=0.174.0 <2.0.0"
  },
  "dependencies": {
    "marked": "latest",
    "katex": "latest",
    "highlight.js": "latest",
    "cheerio": "latest"
  }
}
